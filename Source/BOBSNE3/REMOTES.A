** module to handle remotes deployed by Bob*		REMYSIZE	EQU	6COPTERTIME	EQU	150COPTERFLASH	EQU	120remotecode:	dc.w	lightbulbc	;different remote code	dc.w	shieldc 	dc.w	parachutec	dc.w	trampc	dc.w	copterc	dc.w	bezerkc	aremoteonhead:	dc.b	1		;light bulb	dc.b	1		;shield	dc.b	1		;parachute	dc.b	0		;trampoline	dc.b	1		;copter	dc.b	0		;bezerker	remotepath:			;0=up, 1=down	dc.b	0		;light bulb	dc.b	0		;shield	dc.b	0		;parachute	dc.b	1		;trampoline	dc.b	0		;copter	dc.b	0		;bezerker	activeallowed:	dc.b	1	dc.b	1	dc.b	1	dc.b	1	dc.b	1	dc.b	3	flyupy:	dc.b	-5,-8,-11,-17,-22,-27,-30,-31;	dc.b	-2,-4,-5,-6,-5,-5,-3,-1flydowny:	dc.b	3,7,11,15,20,24,99** deploy active remote*deployremote:	phb	lda	#13	pha	plb		lda	status	cmp	#leavegoth	beq	@norem;	cmp	#windblown;	beq	@norem	cmp	#elevator	beq	@norem	cmp	#drivescooter	beq	@norem	cmp	#crouching	bne	@good@norem:	plb	rtl@good:	lda	remote				;ignore if none	bmi	@norem	jsl	doingwhat2			;if can deploy, then do...	bcc	@norem	lda	dpremflag			;chest cavity must be closed	bne	@norem		lda	remotedelay			;do not allow over crowding	bne	@norem		lda	activeremotes		;3 total at a time	cmp	#3	beq	@norem		ldy	remote				;must have remotes	cpy	#5	bne	@onlyone	lda	perremote,y	cmp	#3	beq	@norem	bra	@morethanone@onlyone:	lda	perremote,y			;one remote at a time	bne	@norem@morethanone:	lda	remotecount,y		;must have remotes	beq	@norem	** conditions satisfied...deploy*	lda	aremoteonhead,y		;one remote per head	beq	@nobother	lda	remoteonhead		;if one already there...	bne	@nodeploy			;then ignore	lda	#1@nobother:		sta	temp	lda	#type_remote	jsl	TSKschedule			;and let loose...	dw	remotelogic	bcc	@nodeploy		tay	lda	#13	sta	TSKbank,y	lda	remote	sta	PIClogic,y	lda #1	sta	PICbank,y	sta	PICprior,y	lda	walkdir			;direction	sta	PICdir,y	lda	#4				;fall speed	sta	PICflag,y	lda	#0	sta	PICed,y	lda	temp	beq	@nohead	sta	PICcount2,y	@nohead:	lda	#16					;delay before next	sta	remotedelay		inc	activeremotes		;inc number acitve		ldy	remote	lda	remotecount,y	dec	a	sta	remotecount,y		;dec number on bob		lda	perremote,y	inc	a	sta	perremote,y			;inc number allowed	inc	dpremflag			lda	#SFXDEPLOYREM	jsl	soundeffects@nodeploy:	plb	rtl** welcome to the land of remotes*remotelogic:	jsr	remoteover	bcc	@fine	lda	status	cmp	#carried	bne	@notcarried	cmp	#dying	beq	@notcarried	lda	#falling	sta	status	lda	#1	sta	jumpyspeed@notcarried:	stz	enemyblind	stz	bobshield	jmp	doexplosion@fine:	lda	PICed,x	bne	@moving		lda	dpremflag	cmp	#3	beq	@letout	cmp	#4	bne	@notyet** wait to let out of chest*@letout:	lda	bobxpos	sta	PICxlo,x		lda	bobypos	sec		sbc	#6	sta	PICylo,x	jsr	drawremote 	inc	PICed,x@notyet:	rtl** else continue with trajectory*@moving:;	lda	PICxhi,x		;kill if off...;	ora	PICyhi,x;	beq	@notoff;	jmp	killremote	@notoff:	lda	#0	sta	PICattr,x		jsr	drawremote		;get next frame of remote		lda	PICstatus,x		;entering or funcitioniong	bne	remoteinaction	jmp	startremote** remote is in action*remoteinaction:	lda	PICcount2,x	beq	@notonhead	stx	remoteonhead@notonhead:	lda	#1	sta	PICscroll,x		lda	PIClogic,x		;execute code...	asl	a	tay	lda	remotecode,y	sta	tmp	iny	lda	remotecode,y	sta	tmp+1		jmp	(tmp)** drift slowly*parachutec:;	stz	PICprior,x	stz	PICscroll,x		lda	#1	sta	bobfloat		lda	#1	sta	PICanim,x		lda	bobmd	bne	@die	lda	status	cmp	#jumping	beq	@alive	cmp	#falling	beq	@alive@die:	lda	#0	sta	bobfloat	jmp	doexplosion@alive:		rtl** flashing light bulb*lightbulbc:	stz	PICscroll,x		inc	enemyblind		;factory time	lda	enemyblind	cmp	#240	bcc	@alive@dead:	stz	enemyblind	lda	#0				;die when over	jmp	doexplo sion@alive:	and	#7	cmp	#7	bcc	@noflash	lda	#SFXLIGHT	jsl	soundeffects@noflash	rtl** body shield*shieldc:	stz	PICscroll,x		lda	bobshield	bne	@alive@dead:	lda	#0				;die when over	sta	bobshield	jmp	doexplosion@alive:	lda	bobshield	cmp	#30	bcs	@fine	and	#2	cmp	#2	bne	@fine	stz	PICadrLo,x	stz	PICadrHi,x@fine:	rtl** trampoline*trampsizey:	dc.b	8,8,10,12,16trampanim:	dc.b	0,1,3,2,3,4,5,6,7,8		trampc:	lda	PICstatus,x	cmp	#1	beq	@lookforbob	** bob has sprung it...*	inc	PICcount,x	lda	PICcount,x	cmp	#10	bcs	@endit	tay	lda	trampanim,y	sta	PICanim,x	lda	#1	sta	PICscroll,x@wait:	rtl@endit:	lda	#0	jmp	doexplosion		** opened...wait for bob to hit*@lookforbob:;	stz	PICprior,x	stz	PICscroll,x	lda	bobxpos	sta	PICxlo,x		lda	bobypos	clc	adc	#24	sta	PICylo,x		lda	PICcount,x	cmp	#3	bcc	@open		lda	#6	sta	jumptype	jsl	startjump2l	;big jump	lda	#SFXSPRING	jsl	soundeffects	lda	#2	sta	PICstatus,x@nolook:	rtl	** open the tramp upon landing*@open:	inc	PICcount,x		lda	PICcount,x	tay	lda	trampanim,y	sta	PICanim,x@fuckoff:	rtl	** destroy all bezerker*bezerkc:;	stz	PICprior,x		lda	#1	sta	PICscroll,x		lda	PICthingy,x	cmp	#12	bcs	@above	inc	PICthingy,x	lda	#-1	jsl	PICyadd	rtl@above:		lda	PICtemp2,x		;if timer expired...	cmp	#4	bcs	@readytodie	** count down the fuse*	lda	PICcount,x		;5 second fuse			cmp	#5	bcs	@explorem	inc	PICcount,x	bra	@noexplode@explorem:	stz	PICcount,x	inc	PICtemp2,x	lda	PICtemp2,x	cmp	#4	bcc	@noexplode	lda	#SFXBEZERKER	jsl	soundeffects	lda	#$e0	sta	screenstate				** vibrate the remote and them implode*@readytodie	inc	PICcount,x		;vibration counter	lda	PICcount,x	cmp	#30	bcs	@yes	and	#1	beq	@down	lda	#2	bne	@updown@down:	lda	#-2@updown:	jsl	PICyadd	rtl** do the implosion*@yes:	lda	#3	sta	enemydie		;signal death for all		lda #30	sta shakescreen		lda #50	sta fuckscreen		;special hdma trick!		lda	#0	jmp	doexplosion@noexplode:		rtl** carrying copter*copterc:	stz	PICscroll,x		;synch with bob		lda	TSKclock	and	#3	cmp	#3	bne	@nocops	lda	#SFXCOPTER	jsl	soundeffects@nocops:		jsr	remotecoll		;touch wall and die....	bcc	@timenotup@crashbob:	lda	status			;ignore if dead	cmp	#carried	bne	@explode	lda	#1	sta	jumpyspeed	lda	#falling		;bob walks...	sta	status	stz	fallcount@explode:	lda	#23		;12	jmp	doexplosion@timenotup:	lda	status	cmp	#carried	bne	@explode		inc	PICcount,x		;factory time 	lda	PICcount,x	cmp	#COPTERTIME	bcs	@crashbob		lda	bobmd			;if bob on ground...	beq	@stillflying		;kill it	lda	#walking	sta	status	bra	@explode@stillflying:	rtl	** remote is deploying*startremote:	stz	temp+7@pastdead:	ldy	PIClogic,x		;get type	lda	remotepath,y	;and what path to take	bne	@godown	** fly the remnote up and out...*	lda	PIClogic,x		;copter can grab bob on way up	cmp	#copterguy	bne	@regular	jsr	copterbob@regular:	lda	bobxpos	sta	PICxlo,x		ldy	PICcount,x	lda	flyupy,y		;up y	clc	adc	bobypos	sta	PICylo,x;	jsl	PICyadd	lda	PICcount,x	inc	a	sta	PICcount,x	cmp	#8	beq	@remotedeployed	rtl@remotedeployed:	inc	PICstatus,x	stz	PICcount,x	lda PIClogic,x	cmp	#1	bne	@tryflash	lda	#255	sta	bobshield	rtl@tryflash:	bne	@ignore	lda	#0	sta	colorval	stz	startcolor	lda	#25	sta	endcolor	stz	colorstrength@ignore:	rtl** remote dropping for ground...*@godown:	lda	bobxpos	sta	PICxlo,x	stz	PICscroll,x	ldy	PICcount,x	lda	flydowny,y	cmp	#99	beq	@remotedeployed	sta	temp	lda	bobypos	sta	PICylo,x	lda	temp	jsl	PICyadd	inc	PICcount,x	rtl			** remote collision checking...*	carry set if collide*remotexsize	dc.b	8,8,16,12,8,8remotecoll:	lda	#0				;check top of remote...	ldy	#-8	jsl	bgcoll_long	jsl	findwall_long	bcc	@tryother	rts@tryother:;	ldy	PIClogic,x;	lda	remotexsize,y	lda	#0	ldy PICdir,x			;which side of remote...	bpl	@pos	eor	#255	clc	adc	#1@pos:	sta	temp+7	ldy	#-8	jsl	bgcoll_long	jsl	findwall_long	rts** see if copter has bob*copterbob:;	lda	PICxlo,x	;hit the centre please;	sec	;	sbc	#8;	cmp	bobxpos;	bcs	@nottrying;	clc;	adc	#16;	cmp	bobxpos;	bcc	@nottrying	;	lda	bobypos;	sec;	sbc	bobsizey;	sec;	sbc	#8;	sta	temp	;	lda	PICylo,x	;and below;	cmp	temp;	bcs	@nottrying;	clc;	adc	#8;	cmp	temp;	bcc	@nottrying** attach to bob*	lda	PICcount,x	cmp	#7	bcc	@nottrying		lda	#2	sta	PICstatus,x		;remote is carrying bob...	lda	#carried	sta	status	sta	PICcount,x	stz	pickupcount	;	lda	bobxpos			;scroll bob along...;	sta	PICxlo,x;	;	lda	bobypos			;y adjust;	sec;	sbc	#34;	sta	PICylo,x		@nottrying:	rts	** remove remote from system* create an explosion*doexplosion:	sta	PICstatus,x			;save size	stz	PICed,x				;counter		lda	PIClogic,x			;and per each	tay		lda	PICcount2,x	beq	@notonhead	stz	remoteonhead@notonhead:	lda	perremote,y	dec	a	sta	perremote,y	dec	activeremotes	lda	#0	sta	TSKbank,x	lda	#23				;12	;PICstatus,x	jml	explode** just remove from system*killremote:	lda	PIClogic,x			;and per each	tay	lda	PICcount2,x	beq	@notonhead	stz	remoteonhead@notonhead:	lda	perremote,y	dec	a	sta	perremote,y	dec	activeremotes		;inc number acitve	jmp	TSKcancel		** draw remote*remotefroff:	db	0,2,4,6,14,16remtwoframer:	db	1,1,0,0,1,0	drawremote:	stz	temp	lda	PIClogic,x		;get type	tay	cmp	#5	bne	@notbez** draw the bezerker*	lda	PICtemp2,x	asl	a	sta	temp	bra	@alive** others...*@notbez:	lda	remtwoframer,y	beq	@otheranim	lda	TSKclock	and	#2	sta	temp	bra	@drawrem		@otheranim:						;other 2 set own animation	lda	PICanim,x	asl	a	sta	temp	@drawrem:	lda	PIClogic,x				;extract frame data	tay	cmp	#4	bne	@alive	lda	PICcount,x	cmp	#COPTERFLASH	bcc	@alive		lda	TSKclock	and	#1	beq	@alive	lda	#0	sta	PICadrLo,x	sta	PICadrHi,x	bra	@flip** finaly draw it*@alive:	lda	remotefroff,y	asl	a	clc	adc	temp	tay	lda	remoteframes,y	sta	tmp	iny	lda	remoteframes,y	sta	tmp+1	jsl	PICput2@flip:	lda	PIClogic,x	cmp	#1	beq	@doflip	cmp	#4	bne	@noflip	lda	walkdir				;get correct dir...	bpl	@noflip@doflip:	jsl	PICxflip@noflip:	rts** should remote cancel itself*remoteover:	lda	waitforwin	bne	@byebye	lda	status	cmp	#dying	beq	@byebye	cmp	#teleported	beq	@byebye	cmp	#sleeping	beq	@byebye	cmp	#drivescooter	beq	@byebye	cmp	#leavegoth	beq	@byebye	clc	rts@byebye:	sec	rts** all the remote picture frames*remoteframes:lightfr:	dw	rem6	dw	rem7shieldfr:	dw	rem8	dw	rem8surffr:	dw	rem19	dw	rem18trampfr:	dw	rem10		;release	dw	rem11	dw	rem12		;flag	dw	rem13	dw	rem14	dw	rem15	dw	rem16	dw	rem17copfr:		dw	rem0	dw	rem1bezfr:	dw	rem21	dw	rem22	dw	rem23	dw	rem24	dw	rem20** teleport swirl from dennis' mouth*teleportswirl:	lda	#6	sta	PICcolor,x	lda	#1	sta	PICscroll,x	lda	#13	sta	PICbank,x		stz	PICtype,x	jsl	TSKsuspend	lda	PICstatus,x	beq	@look	lda	status	cmp	#teleported	bne	@notele	jml	TSKcancel@look:		lda	PICxhi,x	ora	PICyhi,x	bne	@notele		lda	PICxlo,x	cmp	#120	bcc	@notele	cmp	#136	bcs	@notele		lda	PICylo,x	sec	sbc	#50	cmp	bobypos	bcs	@notele	clc	adc	#100	cmp	bobypos	bcc	@notele		jsl	startvictory	inc	PICstatus,x@notele:	lda	PICcount,x	inc	a	sta	PICcount,x	and	#31	asl	a	tay	lda	teleswirlframes,y	sta	tmp	lda	teleswirlframes+1,y	sta	tmp+1	jsl	PICput2	rtlteleswirlframes:	dc.w	teleswirl0	dc.w	teleswirl1	dc.w	teleswirl2	dc.w	teleswirl3	dc.w	teleswirl4	dc.w	teleswirl5	dc.w	teleswirl6	dc.w	teleswirl7	dc.w	teleswirl8	dc.w	teleswirl9	dc.w	teleswirl10	dc.w	teleswirl11	dc.w	teleswirl12	dc.w	teleswirl13	dc.w	teleswirl14	dc.w	teleswirl15	dc.w	teleswirl16	dc.w	teleswirl17	dc.w	teleswirl18	dc.w	teleswirl19	dc.w	teleswirl20	dc.w	teleswirl21	dc.w	teleswirl22	dc.w	teleswirl23	dc.w	teleswirl24	dc.w	teleswirl25	dc.w	teleswirl26	dc.w	teleswirl27	dc.w	teleswirl28	dc.w	teleswirl29	dc.w	teleswirl30	dc.w	teleswirl31teleswirl0: dc.b $0A dc.b $4C,$4B,$7D dc.b $08,$50,$6A dc.b $49,$4C,$6A dc.b $62,$43,$6A dc.b $10,$0F,$8D dc.b $0A,$4A,$8D dc.b $45,